<!DOCTYPE html>
<html lang="ko">
  <head>
    {{>meta}}

    <title>{{title}}</title>

    {{>stylesheet}}
  </head>

  <body class="fixed-nav sticky-footer bg-dark" id="page-top">

    {{>navigator}}

    <div class="content-wrapper">
      <div class="container-fluid">

        <!-- Breadcrumbs -->
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="#">{{menu1}}</a>
          </li>
          {{#if menu2}}
          <li class="breadcrumb-item active">{{menu2}}</li>
          {{/if}}
        </ol>

        {{>body}}

      </div>
      <!-- /.container-fluid -->
    </div>
    <!-- /.content-wrapper -->

    {{>footer}}

    {{>layout-modal}}

    {{>scripts}}
  </body>
</html>

