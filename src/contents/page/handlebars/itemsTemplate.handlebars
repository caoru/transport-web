
<div class="card mb-3">
  <div class="card-header">
    <i class="fa fa-table"></i>
      짐
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" width="100%" id="itemsTable" cellspacing="0">
        <thead>
          <tr>
            <th>번호</td>
            <th>이름</td>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>번호</td>
            <th>이름</td>
          </tr>
        </tfoot>
        <tbody>
          {{#items}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
          </tr>
          {{/items}}
        </tbody>
      </table>
    </div>
  </div>
</div>

